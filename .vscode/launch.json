  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/data/posts/postsPutAll.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "console": "integratedTerminal"
    },

    {
      "type": "node",
      "request": "attach",
      "name": "Next.js Debug",
      "skipFiles": ["<node_internals>/**"],
      "port": 9229,
      "sourceMapPathOverrides": {
        "webpack:///./*": "${workspaceRoot}/nextjs/*"
      }
    }
  ]
}
